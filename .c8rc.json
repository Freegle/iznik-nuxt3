{
  "reporter": ["lcov", "text", "html"],
  "include": [
    "**/*.vue",
    "**/*.js",
    "**/*.ts",
    "**/*.mjs"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/test-results/**",
    "**/coverage/**",
    "**/playwright-report/**",
    "**/.nuxt/**",
    "**/dist/**",
    "**/build/**",
    "**/.output/**",
    "**/playwright.config.js",
    "**/nuxt.config.js",
    "**/tailwind.config.js",
    "**/commitlint.config.js",
    "**/postcss.config.js",
    "**/*.d.ts",
    "**/*.config.*",
    "**/setup-hooks.*",
    "**/run-lint-on-changed.*"
  ],
  "all": true,
  "check-coverage": false,
  "skip-full": false,
  "clean": true,
  "temp-directory": "coverage/tmp",
  "resolve": "coverage/tmp"
}