{
	"appId": "org.ilovefreegle.direct",
	"appName": "Freegle",
	"webDir": ".output/public",
	"bundledWebRuntime": false,
	"cordova": {
		"preferences": {
			"CameraUsesGeolocation": "true",
			"AndroidLaunchMode": "singleTask"
		}
	},
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Badge": {
			"persist": true,
			"autoClear": false
		}
	}
}
