{
  "private": true,
  "scripts": {
    "dev": "nuxi dev --port=3002",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "appprod": "SET APP_ENV=production&&nuxi generate",
    "appdebug": "SET APP_ENV=debug&&nuxi generate",
    "start": "node .output/server/index.mjs",
    "postinstall": "patch-package",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.vue .",
    "lint-staged": "node -e \"const { platform } = require('os'); const { execSync } = require('child_process'); const isMingw = process.env.MSYSTEM && process.env.MSYSTEM.includes('MINGW'); if (platform() === 'win32' && !isMingw) { execSync('run-lint-on-changed.cmd'); } else { execSync('chmod +x ./run-lint-on-changed.sh && sh ./run-lint-on-changed.sh'); }\"",
    "prepare": "node -e \"const { platform } = require('os'); const { execSync } = require('child_process'); const isMingw = process.env.MSYSTEM && process.env.MSYSTEM.includes('MINGW'); if (platform() === 'win32' && !isMingw) { execSync('setup-hooks.cmd'); } else { execSync('chmod +x ./setup-hooks.sh && sh ./setup-hooks.sh'); }\"",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:prod": "TEST_BASE_URL=https://www.ilovefreegle.org playwright test",
    "test:clean": "node -e \"const fs = require('fs'); const rimraf = path => { try { if (fs.existsSync(path)) fs.rmSync(path, { recursive: true, force: true }); } catch (e) { console.error('Error removing:', e); } }; rimraf('test-results'); rimraf('playwright-report'); rimraf('playwright-screenshots');\"",
    "test:ci": "npm run test:clean && playwright test",
    "test:unsubscribe-emails": "node tests/e2e/unsubscribe-test-emails.js"
  },
  "gitHooks": {
    "pre-commit": "npm run lint-staged"
  },
