{
  "private": true,
  "scripts": {
    "dev": "nuxi dev --port=3002",
    "build": "nuxi prepare && nuxi build",
    "generate": "nuxi generate",
    "start": "node .output/server/index.mjs",
    "postinstall": "patch-package",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.vue .",
    "lint-staged": "node -e \"const { platform } = require('os'); const { execSync } = require('child_process'); const isMingw = process.env.MSYSTEM && process.env.MSYSTEM.includes('MINGW'); if (platform() === 'win32' && !isMingw) { execSync('run-lint-on-changed.cmd'); } else { execSync('chmod +x ./run-lint-on-changed.sh && bash ./run-lint-on-changed.sh'); }\"",
    "prepare": "node -e \"const { platform } = require('os'); const { execSync } = require('child_process'); const isMingw = process.env.MSYSTEM && process.env.MSYSTEM.includes('MINGW'); if (platform() === 'win32' && !isMingw) { execSync('setup-hooks.cmd'); } else { execSync('chmod +x ./setup-hooks.sh && bash ./setup-hooks.sh'); }\"",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:prod": "TEST_BASE_URL=https://www.ilovefreegle.org playwrigrht test",
    "test:coverage": "npm run test:clean && playwright test",
    "test:coverage:upload": "npm run test:coverage && npm run coveralls",
    "coveralls": "node scripts/upload-coverage.js",
    "test:clean": "node -e \"const fs = require('fs'); const rimraf = path => { try { if (fs.existsSync(path)) fs.rmSync(path, { recursive: true, force: true }); } catch (e) { console.error('Error removing:', e); } }; rimraf('test-results'); rimraf('playwright-report'); rimraf('playwright-screenshots'); rimraf('coverage');\"",
    "test:ci": "npm run test:clean && playwright test",
    "test:unsubscribe-emails": "node tests/e2e/unsubscribe-test-emails.js",
    "test:show-report": "npx playwright show-report --host 0.0.0.0 --port 9323"
  },
  "gitHooks": {
    "pre-commit": "npm run lint-staged"
  },
  "engines": {
    "node": ">=17.9.1"
  },
